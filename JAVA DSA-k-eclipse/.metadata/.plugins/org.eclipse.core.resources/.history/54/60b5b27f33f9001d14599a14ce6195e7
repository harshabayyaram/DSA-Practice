package com.harsha.patterns;

public class Patterns {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	static void pattern1(int n){
		for(int row = 1;row<=n;row++){
			for(int col = 1;col <=row;col++){
				System.out.print("* ");
			}
			System.out.println();
		}
	}
}
